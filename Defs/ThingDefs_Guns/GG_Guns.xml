<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- Simple Gunsmithing -->

  <ThingDef ParentName="BaseMakeableGunG">
    <defName>GG_Gun_Arquebus</defName>
    <label>caliver</label>
    <description>Possibly the first firearm ever created. It's bulky, deafening, and inaccurate, but boy is that a hefty hunk of iron it hurls.</description>
    <graphicData>
      <texPath>Things/Firarms/Arquebus</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Interact_Rifle</soundInteract>
    <statBases>
      <WorkToMake>12000</WorkToMake>
	  <Mass>4.2</Mass>
      <AccuracyTouch>0.2</AccuracyTouch>
      <AccuracyShort>0.7</AccuracyShort>
      <AccuracyMedium>0.45</AccuracyMedium>
      <AccuracyLong>0.1</AccuracyLong>
      <RangedWeapon_Cooldown>5</RangedWeapon_Cooldown>
    </statBases>
    <costList>
      <Steel>30</Steel>
			<WoodLog>30</WoodLog>
    </costList>
    <verbs>
      <li>
        <verbClass>Verb_Shoot</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <defaultProjectile>GG_Bullet_Musket_Ball_L</defaultProjectile>
        <warmupTime>1.2</warmupTime>
        <range>27</range>
        <soundCast>Shot_ShotGun</soundCast>
        <soundCastTail>GunTail_Heavy</soundCastTail>
        <muzzleFlashScale>22</muzzleFlashScale>
      </li>
    </verbs>
    <tools>
      <li>
        <label>stock</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>13</power>
        <cooldownTime>2.5</cooldownTime>
      </li>
      <li>
        <label>barrel</label>
        <capacities>
          <li>Poke</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
      </li>
    </tools>
  </ThingDef>
	
